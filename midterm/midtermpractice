# Midterm Practice Problem

# 01: Is x an integer?

import math
x = float(input())
if (math.floor(x) == math.ceil(x)):
    print(f"{math.floor(x)} is an integer.")
else:
    print(f"{x} is not an integer.")
    
# 02: Point in Square

print("Upper left corner coordinate:")
x = float(input("  << x axis: "))
y = float(input("  << y axis: "))
dx = float(input("  << Eastern: "))
dy = float(input("  << Southern: "))
print("Enter a coordinate:")
px = float(input("  << x axis: "))
py = float(input("  << y axis: "))

if (x <= px and px <= x+dx) and (y-dy <= py and py <= y):
    print(f">>> ({px:.2f}, {py:.2f}) is inside the rectangle.")
else:
    print(f">>> ({px:.2f}, {py:.2f}) is not inside the rectangle.")
    
# 03: Sawasdee Wun Jun

week = int(input())
hr = int(input())
m = int(input())
# day of the week
if week == 1:
    week = "sunday"
elif week == 2:
    week = "monday"
elif week == 3:
    week = "tuesday"
elif week == 4:
    week = "wednesday"
elif week == 5:
    week = "thursday"    
elif week == 6:
    week = "friday"    
elif week == 7:
    week = "saturday"    
    
# time
m = hr*60 + m

if (241 <= m and m <= 720):
    m = "morning"
elif (721 <= m and m <= 1080):
    m = "afternoon"
elif (1081 <= m and m <= 1320):
    m = "evening"
else:
    m = "night"

print(f"good-{m}-{week}.png")

# 04: Distance

import math
xa, ya, xb, yb = -1,-1,-1,-1
while not (xa == 0 and ya == 0 and xb == 0 and yb == 0):
    print("<<Point a>>")
    xa = int(input("Enter x coordinate: "))
    ya = int(input("Enter y coordinate: "))
    print("<<Point b>>")
    xb = int(input("Enter x coordinate: "))
    yb = int(input("Enter y coordinate: "))
    
    # compute
    if (xa == 0 and ya == 0 and xb == 0 and yb == 0):
        print("Goodbye")
    else:
        ## compute direction
        if (xb == xa and yb > ya):
            direction = "north"
        elif (xb > xa and yb > ya):
            direction = "north-east"
        elif (xb > xa and yb == ya):
            direction = "east"
        elif (xb > xa and yb < ya):
            direction = "south-east"
        elif (xb == xa and yb < ya):
            direction = "south"
        elif (xb < xa and yb < ya):
            direction = "south-west"
        elif (xb < xa and yb == ya):
            direction = "west"
        elif (xb < xa and yb > ya):
            direction = "north-west"
        else:
            direction = "nowhere"
        
        d = math.sqrt((xa-xb)**2 + (ya-yb)**2)
        hor = abs(xb-xa)
        ver = abs(yb-ya)
        
        print(f"""Distance between ({xa}, {ya}) and ({xb}, {yb}):
Euclidean distance is {d:.2f}.
Horizontal distance is {hor}.
Vertical distance is {ver}.
We are going {direction}.""")        

