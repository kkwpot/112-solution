# 06: Right Triangle

a = float(input())
b = float(input())
c = float(input())

isRight = False

if (a**2 + b**2 == c**2) or (b**2 + c**2 == a**2) or (c**2 + a**2 == b**2):
    isRight = True
print(isRight)

# 07: Day in January

startA = int(input())
s = int(input())

start = startA%7
x = s%7

if s > 31 or s <= 0 or startA > 7 or startA <= 0:
    print("ERROR")
elif (x == start%7):
    print("Sunday")
elif (x == (start+1)%7):
    print("Monday")
elif (x == (start+2)%7):
    print("Tuesday")
elif (x == (start+3)%7):
    print("Wednesday")
elif (x == (start+4)%7):
    print("Thursday")
elif (x == (start+5)%7):
    print("Friday")
elif (x == (start+6)%7):
    print("Saturday")

# 08: Day in January 2

day = input()
s = int(input())

if (day == "Sunday"):
    trueDay = s + 0
elif (day == "Monday"):
    trueDay = s + 1
elif (day == "Tuesday"):
    trueDay = s + 2
elif (day == "Wednesday"):
    trueDay = s + 3
elif (day == "Thursday"):
    trueDay = s + 4
elif (day == "Friday"):
    trueDay = s + 5
elif (day == "Saturday"):
    trueDay = s + 6
else:
    trueDay = "ERROR"
    
if (s > 31) or (s<1):
    trueDay = "ERROR"
    
if (trueDay != "ERROR"):       
    if (trueDay%7 == 1):
        trueDay = "Sunday"
    elif (trueDay%7 == 2):
        trueDay = "Monday"
    elif (trueDay%7 == 3):
        trueDay = "Tuesday"
    elif (trueDay%7 == 4):
        trueDay = "Wednesday"
    elif (trueDay%7 == 5):
        trueDay = "Thursday"
    elif (trueDay%7 == 6):
        trueDay = "Friday"
    elif (trueDay%7 == 0):
        trueDay = "Saturday"

print(trueDay)

# 09: Find a, b in which a*b=n and (a+b) is the lowest

import math

ab = int(input())

i = 1
max = 99999

while i <= math.floor(math.sqrt(ab)):
    if (ab % i == 0):
        if (max > i + ab/i):
            max = i + ab/i
    i = i+1
print(int(max))

# 10: Sum odd digits

import math

num = input()

x = num.count('1')*1 + num.count('3')*3 + num.count('5')*5 + num.count('7')*7 + num.count('9')*9

if x == 0:
    print(-1)
else:
    print(x)

# 11: Product even digits

import math

num = input()

x = 0**num.count('0') * 2**num.count('2') * 4**num.count('4') * 6**num.count('6') * 8**num.count('8')

if num.count('2') +  num.count('4') +  num.count('6') +  num.count('8') +  num.count('0') == 0:
    print(-1)
else:
    print(x)

# 12: Craps

firstRound = True

result = ''
memory = 0
last = 0
roll = 0
summ = 0

while True:
    x = int(input())
    y = int(input())
    summ = x+y
    roll = roll + 1
    
    if x < 1 or y < 1 or x > 6 or y > 6:
        print("ERROR")
        roll = roll - 1
        continue
    
    if firstRound:
        firstRound = False
        if x+y == 7 or x+y == 11:
            result = 'W'
            break
        if x+y == 2 or x+y == 3 or x+y == 12:
            result = 'L'
            break
        else:
            memory = x+y
            firstRound = False
    else:
        if x+y == memory:
            result = 'W'
            break
        if x+y == 7:
            result = 'L'
            break
        
print(f"{roll} {summ} {result}")
